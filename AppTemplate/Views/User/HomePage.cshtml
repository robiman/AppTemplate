@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <title>@ViewBag.Title | Intergration Monitoring</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/angularCSS")

    @*@Scripts.Render("~/bundles/jqueryval")*@
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/AdminLTE")

    @Scripts.Render("~/bundles/angularJS")
    @Scripts.Render("~/bundles/modernizr")
    @*@Scripts.Render("~/bundles/AppTemplateAngularAppJS")*@
    <script src="~/AppTemplateAngularApp/rootModule.js"></script>
    @*<script src="~/AppTemplateAngularApp/Controller/Account/HomePageCtrl.js"></script>*@
    <script src="~/AppTemplateAngularApp/Services/notificationService.js"></script>
    <script src="~/AppTemplateAngularApp/Services/membershipService.js"></script>
</head>
<body ng-app="AppTemplate" class="skin-blue sidebar-mini">
    <div class="wrapper">
        @Html.Partial("_header")
        <!-- Left side column. contains the logo and sidebar -->
        @Html.Partial("_sideBarLeft")
        <div class="content-wrapper">
            <div class="">
                <div ui-view>
                </div>
            </div>
        </div>
        @Html.Partial("_footer")
        <!-- Control Sidebar -->
        @Html.Partial("_sideBarRight")
        <!-- /.control-sidebar -->
        <!-- Add the sidebar's background. This div must be placed
             immediately after the control sidebar -->
        <div class="control-sidebar-bg"></div>
    </div><!-- ./wrapper -->
</body>
</html>
