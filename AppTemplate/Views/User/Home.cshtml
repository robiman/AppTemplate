
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Ethiopian e-VISA</title>

    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta name="viewport" content="user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimal-ui" />
    <meta name="apple-mobile-web-app-status-bar-style" content="yes" />
 
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css">
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/bootstrap")    
    @Scripts.Render("~/bundles/jquery")

    @*<link href="~/Content/MenuStyle.css" rel="stylesheet" />*@
    @Scripts.Render("~/bundles/angularJS")     
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/AngularAppJS")    

    <script src="~/Scripts/bootstrap.js"></script>
    <script src="~/Scripts/Moto/jquery.min.js"></script>    

    <script type="application/x-javascript"> ddEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700,800' rel='stylesheet' type='text/css'>
    @*<script src="~/eVISAAngularApp/app.js"></script>*@
</head>

<body ng-app="EVisaApp" class="scrollable">
    <div class="scrollable-content">
        <div role="banner">
            @Html.Partial("Header")
        </div>

        <div class="">
            <div ui-view>
            </div>
        </div>

        <div class="" role="contentinfo">
            @Html.Partial("Footer")
        </div>
    </div>
</body>
</html>



